@tailwind base;
@tailwind components;
@tailwind utilities;

:root {
    touch-action: pan-x pan-y;
    height: 100%;
}

label{
    margin-bottom : 0.5em;
}

body{ 
    @apply pl-1 pr-1;
    font-size: 1.2em;
    background-color : #dedede;
}

br { 
    height: 1em;
} 


#page{
    @apply bg-tertiary-200 p-1 mt-2 rounded-md;
}
